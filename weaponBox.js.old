class WeaponBox extends Weapon {
    constructor(location, angle, color) {
        super(location, angle, color);
        this.udpGun = new UdpGun(location, angle, color);
        this.scanner = new Scanner(location, angle, color);
    }

    shoot(type) {
        this[type].shoot();
    }

    render() {
        super.render();
    }
}